{%- for variant in product.variants -%}
	{% case forloop.index %}
		{% when 2 %}
			{% assign daCost = variant.price | money %}
			{% assign monthlyItemPrice = variant.price | money %}
			{% assign savePercent = product.variants[0].price | minus: variant.price | times: 100.0 | divided_by: product.variants[0].price | round %}
			{% assign monthlyPriceinfo = "Monthly plan<br /><span class='paid'>" | append: daCost | append: " paid monthly</span><div class='wrap-save'><span class='save'>Save " | append: savePercent | append: "%</span><span class='permo'>"  | append: monthlyItemPrice | append: "/mo</span></div>" %}
		{% when 3 %}
			{% assign daCost = variant.price | money %}
			{% assign quarterlyItemPrice = variant.price | divided_by: 3 | money %}
			{% assign quarterlyItemPriceNoMoney = variant.price | divided_by: 3 %}
			{% assign savePercent = product.variants[0].price | minus: quarterlyItemPriceNoMoney | times: 100.0 | divided_by: product.variants[0].price | round %}
			{% assign quarterlyPriceinfo = "3 month plan<br /><span class='paid'>" | append: daCost | append: " paid quarterly</span><div class='wrap-save'><span class='save'>Save " | append: savePercent | append: "%</span><span class='permo'>"  | append: quarterlyItemPrice | append: "/mo</span></div>" %}
		{% when 4 %}
			<!-- UUU yup when 4 -->
			{% assign daCost = variant.price | money %}
			{% assign yearlyItemPrice = variant.price | divided_by: 12 | money %}
			{% assign yearlyItemPriceNoMoney = variant.price | divided_by: 12 %}
			{% assign savePercent = product.variants[0].price | minus: yearlyItemPriceNoMoney | times: 100.0 | divided_by: product.variants[0].price | round %}
			{% assign yearlyPriceinfo = "12 month plan<br /><span class='paid'>" | append: daCost | append: " paid annually</span><div class='wrap-save'><span class='save'>Save " | append: savePercent | append: "%</span><span class='permo'>"  | append: yearlyItemPrice | append: "/mo</span></div>" %}
	{% endcase %}
{% endfor %}
