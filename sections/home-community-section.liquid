        "id": "title",
        "label": "Title"
      },
{
        "type": "html",
        "id": "content",
        "label": "Content"
      }
],
  "blocks": [
    {
      "type": "community",
      "name": "Community",
      "settings": [
        

{
        "type": "image_picker",
        "id": "image",
        "label": "Image"
      }
        
      ]
    }
  ],
  "presets": [
    {
      "name": "Home Community Section",
      "category": "Home Section"
    }
  ]
  }
{% endschema %}

